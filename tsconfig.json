{
    "compilerOptions": {
      "outDir": "./dist",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "ESNext",
      "target": "es5",
      "jsx": "react",
      "esModuleInterop":true,
      "moduleResolution":"node",
    },
    "baseUrl": ".",
    "paths": {
      "@/**/*": [
        "./src/**/*"
      ]
    },
    "include": [
      "./src/**/*"
    ]
  }